<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="TiendaZapatosGen.Infraestructure"
		                   namespace="TiendaZapatosGen.Infraestructure.EN.TiendaZapatos">
		    
		    
			
				<class name="AjustesNH" table="Ajustes"> 
				  	
						<id name="Id" column="idAjustes" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
			
			
			
		  			
						<bag name="Usuario" table="[ajustes_usuario]"  >
							<key>
								
									<column name="FK_idAjustes_idAjustes"/> 
							</key>
      						<many-to-many class="TiendaZapatosGen.Infraestructure.EN.TiendaZapatos.UsuarioNH">
									<column name="FK_email_idUsuario"/> 
      						</many-to-many>
    					</bag>
					
					
		
		 </class>	
		  
		  <query name="AjustesNHReadAllHQL">from AjustesNH</query>
		</hibernate-mapping>
	