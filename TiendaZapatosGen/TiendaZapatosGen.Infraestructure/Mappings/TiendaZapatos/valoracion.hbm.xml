<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="TiendaZapatosGen.Infraestructure"
		                   namespace="TiendaZapatosGen.Infraestructure.EN.TiendaZapatos">
		    
		    
			
				<class name="ValoracionNH" table="Valoracion"> 
						<id name="IdValoracion" column="idValoracionValoracion"/> 
			
					
					<property name="Valoracion" type="integer"> 
						<column name="valoracion"  not-null="true"/>
					</property>
					<property name="Comentario" type="string"> 
						<column name="comentario"  not-null="true"/>
					</property>
			
			
			
			
			
		  			
						<many-to-one name="Valoracion_1" class="TiendaZapatosGen.Infraestructure.EN.TiendaZapatos.UsuarioNH" >	
								<column name="FK_email_idValoracion_1"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Producto" class="TiendaZapatosGen.Infraestructure.EN.TiendaZapatos.ProductoNH" >	
								<column name="FK_idProductoProducto_idProducto"/> 
						</many-to-one>
											 					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="ValoracionNHReadAllHQL">from ValoracionNH</query>
		</hibernate-mapping>
	